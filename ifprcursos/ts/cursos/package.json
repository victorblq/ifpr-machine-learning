{
  "name": "cursos",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "ng serve --proxy-config proxy.conf.json",
    "ng": "ng",
    "build": "ng build -d static/ && python ../../../manage.py collectstatic -i *.html --noinput && find ../../templates -name '*.js' -type f -delete && find ../../templates -name '*.map' -type f -delete && find ../../templates -name '*.woff' -type f -delete && find ../../templates -name '*.woff2' -type f -delete && find ../../templates -name '*.ttf' -type f -delete && find ../../templates -name '*.eot' -type f -delete",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.0.2",
    "@angular/cdk": "5.0.0-rc.1",
    "@angular/common": "5.0.2",
    "@angular/compiler": "5.0.2",
    "@angular/core": "5.0.2",
    "@angular/flex-layout": "2.0.0-beta.11",
    "@angular/forms": "5.0.2",
    "@angular/http": "5.0.2",
    "@angular/material": "5.0.0-rc.1",
    "@angular/platform-browser": "5.0.2",
    "@angular/platform-browser-dynamic": "5.0.2",
    "@angular/router": "5.0.2",
    "@covalent/core": "^1.0.0-rc.0",
    "core-js": "2.4.1",
    "rxjs": "^5.4.1",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.2.6",
    "@angular/compiler-cli": "5.0.2",
    "@angular/language-service": "5.0.2",
    "@types/node": "6.0.60",
    "codelyzer": "3.0.1",
    "ts-node": "3.0.4",
    "tslint": "5.3.2",
    "typescript": "2.6.1"
  }
}
